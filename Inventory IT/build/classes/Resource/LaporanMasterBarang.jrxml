	<queryString>
		<![CDATA[SELECT `JenisBarang` AS 'Jenis Barang', `KategoriBarang` AS 'Kategori Barang', `NamaBarang` AS 'Nama Barang', IFNULL(SUM(`Stok`),0) AS 'Stok', `Garansi`, `Keterangan` FROM (SELECT `IdBarang`, `JenisBarang`, `KategoriBarang`, `NamaBarang`, 0 AS 'Stok', `Garansi`, A.`Keterangan` FROM `tbmbarang` AS A JOIN `tbmkategoribarang` AS B ON A.`IdKategoriBarang`=B.`IdKategoriBarang` JOIN `tbmjenisbarang` AS C ON B.`IdJenisBarang`=C.`IdJenisBarang` UNION ALL SELECT `IdBarang`, NULL, NULL, NULL, IFNULL(SUM(`JumlahBarang`),0), NULL, NULL FROM `tbbarangmasukdetail` GROUP BY `IdBarang` UNION ALL SELECT `IdBarang`, NULL, NULL, NULL, IFNULL(SUM(`JumlahBarang`),0)*-1, NULL, NULL FROM `tbbarangkeluar` GROUP BY `IdBarang` UNION ALL SELECT `IdBarang`, NULL, NULL, NULL, IFNULL(SUM(`PenyesuaianStok`),0), NULL, NULL FROM `tbpenyesuaianstok` GROUP BY `IdBarang`) AS `tbmbarangtemp` WHERE 1$P!{Where} GROUP BY `IdBarang` ORDER BY `JenisBarang`, `KategoriBarang`, `NamaBarang`]]>
